{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",

    "allowJs": true,
    "checkJs": false,

    "types": ["vite/client"],
    "skipLibCheck": true,
    "noEmit": true,

    "strict": false
  },
  "include": ["src"]
}